    "import glob\n",
    "import os\n",
    "import pandas as pd\n",
    "\n",
    "all_stock_df_list = []\n",
    "for filename in glob.glob(os.getcwd() +\"/data/raw/\"+\"*.csv\"):\n",
    "    x = pd.read_csv(filename, low_memory=False)\n",
    "    all_stock_df_list.append(x)\n",
    "\n",
    "df = pd.concat(all_stock_df_list)\n",
    "df.reset_index(inplace=True, drop=True)\n",
    "df"
   ]
  }
